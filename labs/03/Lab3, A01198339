%%

"a"|"the"                   { return ARTICLE; }
"boy"|"girl"|"flower"       { return NOUN; }
"touches"|"likes"|"sees"    { return VERB; }
"with"                      { return PREP; }

[ \t]                       { /* ignorar whitespace */ }

