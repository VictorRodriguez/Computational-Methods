%{

#include <stdio.h>

%}



%%

\/\/.*         { printf("COMMENT\n"); }

[ \t\n]+       ; // Ignorar espacios en blanco

float         { printf("floatdcl "); }

int           { printf("intdcl "); }

[a-zA-Z][a-zA-Z0-9]*   { printf("id "); }

[0-9]+        { printf("inum "); }

[0-9]+"."[0-9]+ { printf("fnum "); }

=             { printf("assign "); }

\+            { printf("plus "); }



.             ; // Cualquier otro caracter no reconocido

%%



int yywrap() {

    return 1;

}
int main() {

    yylex();

    return 0;

}
